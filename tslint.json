{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "objects": "always",
                    "arrays": "always",
                    "functions": "never",
                    "typeLiterals": "ignore"
                },
                "esSpecCompliant": true
            }
        ],
        "typedef": [
            true,
            "call-signature",
            "arrow-parameter",
            "parameter",
            "member-variable-declaration"
        ],
        "no-console": false,
        "no-var-requires": false
    },
    "rulesDirectory": []
}
